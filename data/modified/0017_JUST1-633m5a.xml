            <Coords points="1084,648 0,648 0,4 1084,4" />
            <TextLine id="l" custom="readingOrder {index:0;}">
                <Coords points="27,47 206,47 440,47 745,49 1073,57 1073,27 745,19 440,17 206,17 27,17" />
                <Baseline points="27,40 206,40 440,40 745,42 1073,50" />
                <TextEquiv>
                    <Unicode>&#182; Assisa venit recognitura si Emma filia Onorii Malerbe de Norhampton, Thomas filius Simonis filii Iohannis atte Grene </Unicode>
                </TextEquiv>
            </TextLine>
            <TextLine id="l_20" custom="readingOrder {index:1;}">
                <Coords points="26,83 168,83 368,85 599,87 852,89 1078,95 1078,65 852,59 599,57 368,55 168,53 26,53" />
                <Baseline points="26,76 168,76 368,78 599,80 852,82 1078,88" />
                <TextEquiv>
                    <Unicode>de Duston Iohannes filius Iohannis atte Grene de Duston, et Galfridus de Hoghton de Duston iniuste etc. disseisiuerunt Nicholaum </Unicode>
                </TextEquiv>
            </TextLine>
            <TextLine id="l_34" custom="readingOrder {index:2;}">
                <Coords points="26,118 210,118 469,123 749,126 1068,135 1068,105 749,96 469,93 210,88 26,88" />
                <Baseline points="26,111 210,111 469,116 749,119 1068,128" />
                <TextEquiv>
                    <Unicode>Selyman de Flore et Aliciam vxorem eius de libero tenemento suo in Flore post primam etc. Et vnde queruntur quod disseisiuerunt eos </Unicode>
                </TextEquiv>
            </TextLine>
            <TextLine id="l_46" custom="readingOrder {index:3;}">
                <Coords points="27,151 214,160 470,157 470,127 214,130 27,121" />
                <Baseline points="27,144 214,153 470,150" />
                <TextEquiv>
                    <Unicode>de vno mesuagio et sex acris terre cum pertinenciis etc.</Unicode>
                </TextEquiv>
            </TextLine>
            <TextLine id="l_54" custom="readingOrder {index:4;}">
                <Coords points="28,188 186,190 389,190 571,192 808,198 969,207 1063,211 1063,181 969,177 808,168 571,162 389,160 186,160 28,158" />
                <Baseline points="28,181 186,183 389,183 571,185 808,191 969,200 1063,204" />
                <TextEquiv>
                    <Unicode>Et Emma venit. Et alii non veniunt set quidam lohannes de Duston venit et pro omnibus preterquam pro predicta Emma respondet </Unicode>
                </TextEquiv>
            </TextLine>
            <TextLine id="l_70" custom="readingOrder {index:5;}">
                <Coords points="31,218 185,224 361,224 513,225 757,231 928,239 1080,249 1080,219 928,209 757,201 513,195 361,194 185,194 31,188" />
                <Baseline points="31,211 185,217 361,217 513,218 757,224 928,232 1080,242" />
                <TextEquiv>
                    <Unicode>tanquam eorum balliuus et dicit quod ipsi nichil habent in predictis tenementis ad presens nec aliquam inde fecerunt iniuriam seu disseisi</Unicode>
                </TextEquiv>
            </TextLine>
            <TextLine id="l_86" custom="readingOrder {index:6;}">
                <Coords points="33,253 187,254 387,255 387,225 187,224 33,223" />
                <Baseline points="33,246 187,247 387,248" />
                <TextEquiv>
                    <Unicode>nam Et de hoc ponunt se super assisam etc.</Unicode>
                </TextEquiv>
            </TextLine>
            <TextLine id="l_94" custom="readingOrder {index:7;}">
                <Coords points="34,285 241,287 419,294 623,302 810,306 955,314 1069,322 1069,292 955,284 810,276 623,272 419,264 241,257 34,255" />
                <Baseline points="34,278 241,280 419,287 623,295 810,299 955,307 1069,315" />
                <TextEquiv>
                    <Unicode>Et Emma vt tenens etc. respondet et dicit quod predicti Nicholaus et Alicia iniuste tulerunt assisam istam versus eam. </Unicode>
                </TextEquiv>
            </TextLine>
            <TextLine id="l_112" custom="readingOrder {index:8;}">
                <Coords points="29,316 181,321 371,325 516,328 789,340 978,352 1081,359 1081,329 978,322 789,310 516,298 371,295 181,291 29,286" />
                <Baseline points="29,309 181,314 371,318 516,321 789,333 978,345 1081,352" />
                <TextEquiv>
                    <Unicode>Quia dicit quod predicta tenementa quondam fuerunt in seisina quorundam Onorii Malerbe et Iohanne vxoris eius matris predicte Emme </Unicode>
                </TextEquiv>
            </TextLine>
            <TextLine id="l_128" custom="readingOrder {index:9;}">
                <Coords points="27,349 162,353 404,360 619,367 820,372 1060,390 1060,360 820,342 619,337 404,330 162,323 27,319" />
                <Baseline points="27,342 162,346 404,353 619,360 820,365 1060,383" />
                <TextEquiv>
                    <Unicode>cuius heres ipsa est vt de iure ipsius Iohanne. Qui quidem Onorius et Iohanna tenementa illa dimiserunt quibusdam </Unicode>
                </TextEquiv>
            </TextLine>
            <TextLine id="l_142" custom="readingOrder {index:10;}">
                <Coords points="24,384 161,384 348,394 588,400 868,412 1068,423 1068,393 868,382 588,370 348,364 161,354 24,354" />
                <Baseline points="24,377 161,377 348,387 588,393 868,405 1068,416" />
                <TextEquiv>
                    <Unicode>Roberto Michel et Alesie vxori eius tenenda ad terminum vite eorundem Roberti et Alesie. Post quorum mortem et eciam mortem </Unicode>
                </TextEquiv>
            </TextLine>
            <TextLine id="l_156" custom="readingOrder {index:11;}">
                <Coords points="20,418 250,425 501,429 767,440 1046,457 1046,427 767,410 501,399 250,395 20,388" />
                <Baseline points="20,411 250,418 501,422 767,433 1046,450" />
                <TextEquiv>
                    <Unicode>eorundem Onori et lohanne eadem Emma vt filia et heres predicte Iohanne intrauit in tenementis predictis vt in reuersione </Unicode>
                </TextEquiv>
            </TextLine>
            <TextLine id="l_168" custom="readingOrder {index:12;}">
                <Coords points="22,453 235,456 431,461 677,470 913,482 1055,495 1055,465 913,452 677,440 431,431 235,426 22,423" />
                <Baseline points="22,446 235,449 431,454 677,463 913,475 1055,488" />
                <TextEquiv>
                    <Unicode>sua sicut ei bene licit et absque aliqua disseisina eisdem Nicholao et Alicie xori eius facienda. Et de hoc ponit </Unicode>
                </TextEquiv>
            </TextLine>
            <TextLine id="l_182" custom="readingOrder {index:13;}">
                <Coords points="23,492 164,490 338,495 595,506 818,518 1079,531 1079,501 818,488 595,476 338,465 164,460 23,462" />
                <Baseline points="23,485 164,483 338,488 595,499 818,511 1079,524" />
                <TextEquiv>
                    <Unicode>se super assisam. Et predicti Nicholaus et Alicia vxor eius similiter. Ideo capiatur assisa. --- Iuratores dicunt super sacramentum </Unicode>
                </TextEquiv>
            </TextLine>
            <TextLine id="l_196" custom="readingOrder {index:14;}">
                <Coords points="20,525 241,524 480,535 677,540 863,556 1065,570 1065,540 863,526 677,510 480,505 241,494 20,495" />
                <Baseline points="20,518 241,517 480,528 677,533 863,549 1065,563" />
                <TextEquiv>
                    <Unicode>suum quod predicta Emma que nominatur sola die impetracionis predicti breuis, scilicet vicesimo tercio die Augusti vltimo preterito</Unicode>
                </TextEquiv>
            </TextLine>
            <TextLine id="l_210" custom="readingOrder {index:15;}">
                <Coords points="17,561 234,561 458,569 635,574 887,600 1064,608 1064,578 887,570 635,544 458,539 234,531 17,531" />
                <Baseline points="17,554 234,554 458,562 635,567 887,593 1064,601" />
                <TextEquiv>
                    <Unicode>habuit virum superstitem et adhuc habet, scilicet predictum Thomam filium Simonis filii Iohannis atte Grene de Duston nomine. </Unicode>
                </TextEquiv>
            </TextLine>
            <TextLine id="l_224" custom="readingOrder {index:16;}">
                <Coords points="23,588 204,591 414,598 617,610 786,621 942,636 1073,644 1073,614 942,606 786,591 617,580 414,568 204,561 23,558" />
                <Baseline points="23,581 204,584 414,591 617,603 786,614 942,629 1073,637" />
                <TextEquiv>
                    <Unicode>Et quia hoc compertum est per assisam istam, ideo predicta Emma et alii inde sine die et predicti Nicholaus et Alicia nichil </Unicode>
                </TextEquiv>
            </TextLine>
            <TextLine id="l_242" custom="readingOrder {index:17;}">
                <Coords points="23,620 170,625 358,635 570,644 570,614 358,605 170,595 23,590" />
                <Baseline points="23,613 170,618 358,628 570,637" />
                <TextEquiv>
                    <Unicode>capiant per breue suum set sint in misericordia pro falso clamore etc.</Unicode>
                </TextEquiv>
            </TextLine>
